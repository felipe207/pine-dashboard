{
    "name": "brediweb/bredi-dashboard",
    "description": "Pacote dashboard para laravel 8",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilvini",
            "email": "ilvini.pitter@hotmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Brediweb\\BrediDashboard8\\Providers\\BrediDashboardServiceProvider",
                "Brediweb\\BrediDashboard8\\Providers\\BrediColorAdminServiceProvider"
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Brediweb\\BrediDashboard8\\": "src/"
        },
        "files": ["src/Http/Helper/Helper.php"]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan jetstream:install livewire"
        ]
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/jetstream": "^2.3",
        "spatie/laravel-permission": "^5.3",
        "spatie/laravel-sluggable": "^2.6",
        "barryvdh/laravel-debugbar": "^3.2",
        "laravelcollective/html": "^6.2.1",
        "livewire/livewire": "^2.5",
        "arcanedev/log-viewer": "^8.2.0",
        "vinkla/hashids": "^9.1.0"
    }
}
